{
  "name": "gulp-bundle-webtask",
  "description": "Gulp plugin for browserifying and bundling your source code before deploying to webtask.io.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/gulp-bundle-webtask",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/gulp-bundle-webtask",
  "bugs": {
    "url": "https://github.com/doowb/gulp-bundle-webtask/issues"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "browserify": "^13.1.0",
    "data-store": "^0.16.1",
    "date-store": "^0.1.2",
    "extend-shallow": "^2.0.1",
    "find-pkg": "^0.1.2",
    "gulp-uglify": "^2.0.0",
    "lazy-cache": "^2.0.1",
    "simple-get": "^2.2.2",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.10",
    "gulp-istanbul": "^1.1.0",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.0.2"
  },
  "keywords": [
    "assemble",
    "browserify",
    "bundle",
    "generate",
    "gulp",
    "gulp-plugin",
    "gulp-uglify",
    "gulpplugin",
    "plugin",
    "uglify",
    "verb",
    "webtask"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "browserify",
      "gulp-uglify",
      "verb",
      "verb-generate-readme",
      "webtask",
      "assemble",
      "generate",
      "gulp"
    ]
  }
}
